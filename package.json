    "nprogress": "^0.2.0",
    "pluralize": "^8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-paginate": "^8.1.3",
    "reflect-metadata": "^0.1.13",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-parse": "^8.0.4",
    "rehype-shiki": "^0.0.9",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
