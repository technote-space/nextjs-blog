{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "build": "pathpida && next build && next export",
    "start": "next start",
    "lint": "next lint",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "serve": "cd out && http-server -o"
  },
  "dependencies": {
    "autoprefixer": "^10.3.4",
    "dayjs": "^1.10.7",
    "gray-matter": "^4.0.3",
    "next": "^11.1.2",
    "postcss": "^8.3.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark": "^14.0.1",
    "remark-html": "^14.0.1",
    "tailwindcss": "^2.2.15"
  },
  "devDependencies": {
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.20",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "http-server": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.17.0",
    "typescript": "^4.4.3"
  }
}
