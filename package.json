{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "build": "pathpida && next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "serve": "cd out && http-server -o",
    "prepare": "husky install && pathpida"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "dayjs": "^1.10.7",
    "framer-motion": "^4",
    "gray-matter": "^4.0.3",
    "next": "^11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "remark": "^14.0.1",
    "remark-html": "^14.0.1",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.20",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "http-server": "^13.0.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.17.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=15",
    "yarn": "^1",
    "npm": "use_yarn_instead"
  }
}
