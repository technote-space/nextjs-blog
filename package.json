{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "build": "pathpida && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "cover": "jest",
    "cover:update": "jest -u",
    "test": "yarn lint && yarn typecheck && yarn cover",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "serve": "next export && cd out && http-server -o",
    "prepare": "npm_config_yes=true npx shx cp -u .env.example .env && npm_config_yes=true npx shx cp -u src/config/settings.example.ts src/config/settings.ts && husky install && pathpida",
    "sb": "start-storybook -p 6006",
    "build-sb": "build-storybook",
    "analyze": "pathpida && ANALYZE=true next build"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "dayjs": "^1.10.7",
    "fast-average-color-node": "^1.0.3",
    "framer-motion": "^4",
    "gray-matter": "^4.0.3",
    "html-to-text": "^8.0.0",
    "next": "^11.1.2",
    "node-canvas": "^2.7.0",
    "prismjs": "^1.25.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-paginate": "^7.1.3",
    "reflect-metadata": "^0.1.13",
    "rehype-stringify": "^9.0.2",
    "remark": "^14.0.1",
    "remark-gfm": "^2.0.0",
    "remark-rehype": "^9.0.0",
    "remove-markdown": "^0.3.0",
    "serverless-mysql": "^1.5.4",
    "simple-react-lightbox": "^3.6.9-0",
    "tsyringe": "^4.6.0",
    "url-metadata": "^2.5.0",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@next/bundle-analyzer": "^11.1.2",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storyshots": "^6.3.8",
    "@storybook/builder-webpack5": "^6.3.8",
    "@storybook/manager-webpack5": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/gtag.js": "^0.0.7",
    "@types/html-to-text": "^8.0.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/prismjs": "^1.16.6",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-paginate": "^7.1.1",
    "@types/remove-markdown": "^0.3.1",
    "@types/simple-react-lightbox": "^3.6.1",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "babel-jest": "^27.2.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-prismjs": "^2.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "esbuild": "^0.12.28",
    "esbuild-jest": "^0.5.0",
