{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "build": "pathpida && next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "cover": "jest",
    "test": "yarn lint && yarn typecheck && yarn cover",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "serve": "cd out && http-server -o",
    "prepare": "husky install && pathpida",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "dayjs": "^1.10.7",
    "framer-motion": "^4",
    "gray-matter": "^4.0.3",
    "html-to-text": "^8.0.0",
    "next": "^11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "remark": "^14.0.1",
    "remark-html": "^14.0.1",
    "remove-markdown": "^0.3.0",
    "serverless-mysql": "^1.5.4",
    "tsyringe": "^4.6.0",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storyshots": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/html-to-text": "^8.0.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.21",
    "@types/remove-markdown": "^0.3.1",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "babel-jest": "^27.2.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-unused-imports": "^1.1.4",
    "http-server": "^13.0.2",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "mockdate": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.17.0",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-performance": "^0.16.1",
    "storybook-builder-vite": "^0.0.12",
    "typescript": "^4.4.3",
    "vite": "^2.5.8"
  },
  "engines": {
    "node": ">=14",
    "yarn": "^1",
    "npm": "use_yarn_instead"
  }
