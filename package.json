{
  "name": "my-app",
  "version": "0.9.2",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "build": "pathpida && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "cover": "jest --coverage",
    "cover:update": "jest -u",
    "test": "yarn lint && yarn typecheck && yarn cover",
    "update": "npm_config_yes=true npx npm-check-updates -x eslint -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "serve": "next export && cd out && http-server -o",
    "prepare": "if [ ! -f .env ]; then cp .env.example .env; fi && if [ ! -f src/config/settings.ts ]; then cp src/config/settings.example.ts src/config/settings.ts; fi && husky install && pathpida",
    "sb": "start-storybook -p 6006 -s ./public",
    "build-sb": "build-storybook -s public",
    "analyze": "pathpida && ANALYZE=true next build"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.4",
    "@emotion/react": "^11",
